{
    "name": "myklon/brackets-checker",
    "type": "library",
    "description": "A simple library for validating balanced brackets in a string",
    "keywords": ["brackets"],
    "homepage": "https://github.com/Myklon/brackets-checker",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Myklon",
            "email": "keironov@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "phpunit/phpunit": "10.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Myklon\\BracketsChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Myklon\\BracketsChecker\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
